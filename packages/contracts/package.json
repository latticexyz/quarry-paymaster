{
  "name": "contracts",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "mud build",
    "clean": "forge clean && shx rm -rf src/**/codegen",
    "deploy:garnet": "mud deploy --profile=garnet",
    "deploy:local": "mud deploy",
    "deploy:redstone": "mud deploy --profile=redstone",
    "deploy:rhodolite": "mud deploy --profile=rhodolite",
    "dev": "mud dev-contracts",
    "lint": "pnpm run prettier && pnpm run solhint",
    "prettier": "prettier --write 'src/**/*.sol'",
    "solhint": "solhint --config ./.solhint.json 'src/**/*.sol' --fix",
    "test": "tsc --noEmit && mud test",
    "fund:paymaster:local": "forge script FundPaymaster --broadcast --sig \"run(address)\" --rpc-url http://127.0.0.1:8545 -vvv $(jq -r '.[\"31337\"].address' worlds.json)",
    "fund:service:local": "forge script FundService --broadcast --sig \"run(address)\" --rpc-url http://127.0.0.1:8545 -vvv $(jq -r '.[\"31337\"].address' worlds.json)",
    "fund:service:rhodolite": "forge script FundService --broadcast --sig \"run(address)\" --rpc-url https://rpc.rhodolitechain.com -vvv $(jq -r '.[\"17420\"].address' worlds.json)",
    "fund:paymaster:rhodolite": "forge script FundPaymaster --broadcast --sig \"run(address)\" --rpc-url https://rpc.rhodolitechain.com -vvv $(jq -r '.[\"17420\"].address' worlds.json)"
  },
  "dependencies": {
    "@latticexyz/cli": "2.2.11-main-916aa8b346f98a490fee1c25119e91d94ee18ef6",
    "@latticexyz/schema-type": "2.2.11-main-916aa8b346f98a490fee1c25119e91d94ee18ef6",
    "@latticexyz/store": "2.2.11-main-916aa8b346f98a490fee1c25119e91d94ee18ef6",
    "@latticexyz/world": "2.2.11-main-916aa8b346f98a490fee1c25119e91d94ee18ef6",
    "@latticexyz/world-modules": "2.2.11-main-916aa8b346f98a490fee1c25119e91d94ee18ef6"
  },
  "devDependencies": {
    "@account-abstraction/contracts": "^0.7.0",
    "@openzeppelin/contracts": "^5.0.0",
    "@types/node": "^18.15.11",
    "ds-test": "https://github.com/dapphub/ds-test.git#e282159d5170298eb2455a6c05280ab5a73a4ef0",
    "forge-std": "https://github.com/foundry-rs/forge-std.git#74cfb77e308dd188d2f58864aaf44963ae6b88b1",
    "prettier": "3.2.5",
    "prettier-plugin-solidity": "1.3.1",
    "solhint": "^3.3.7",
    "solhint-config-mud": "2.2.11-main-916aa8b346f98a490fee1c25119e91d94ee18ef6",
    "solhint-plugin-mud": "2.2.11-main-916aa8b346f98a490fee1c25119e91d94ee18ef6"
  }
}
